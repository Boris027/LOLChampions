<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main.xml"
    app:startDestination="@id/taskListFragment">

    <fragment
        android:id="@+id/taskListFragment"
        android:name="com.turing.alan.cpifp.ui.TaskListFragment"
        android:label="fragment_task_list"
        tools:layout="@layout/fragment_task_list" >
        <action
            android:id="@+id/action_taskListFragment_to_championdetail"
            app:destination="@id/championdetail" />
    </fragment>
    <fragment
        android:id="@+id/championdetail"
        android:name="com.turing.alan.cpifp.ui.championdetail"
        android:label="championdetail" >
        <argument
            android:name="idchampion"
            app:argType="integer" />
    </fragment>
</navigation>